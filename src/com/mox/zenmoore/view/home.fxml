<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="homePane" prefHeight="614.0" prefWidth="1073.0" style="-fx-background-color: #F5F5F5;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mox.zenmoore.controller.Controller_home">
   <children>
      <Label fx:id="homelabel" layoutX="170.0" layoutY="33.0" onMouseClicked="#setToday" prefHeight="71.0" prefWidth="123.0" text="今日">
         <font>
            <Font name="Dotum" size="59.0" />
         </font>
      </Label>
      <Button fx:id="btn_display" layoutX="433.0" layoutY="51.0" mnemonicParsing="false" onAction="#showDisplay" prefHeight="36.0" prefWidth="95.0" style="-fx-text-fill: black; -fx-border-color: black; -fx-background-color: #FFFAFA; -fx-border-radius: 5px;" text="Display">
         <font>
            <Font name="FZYaoTi" size="18.0" />
         </font>
      </Button>
      <Separator layoutX="533.0" orientation="VERTICAL" prefHeight="614.0" prefWidth="55.0" />

      <ComboBox fx:id="srb" blendMode="SRC_ATOP" editable="true" layoutX="611.0" layoutY="51.0" onAction="#showSearchResult" prefHeight="36.0" prefWidth="418.0" promptText="这可能是一个假的搜索框" style="-fx-background-color: #F5F5F5; -fx-border-color: #F5F5F5;">
         <effect>
            <DropShadow blurType="GAUSSIAN" color="FLORALWHITE" height="0.0" radius="0.0" width="0.0" />
         </effect></ComboBox>
      <ImageView fx:id="notionImage" fitHeight="195.0" fitWidth="184.0" layoutX="603.0" layoutY="133.0" onMouseClicked="#showNotions" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@FXMLImage/notion.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="186.0" fitWidth="207.0" layoutX="896.0" layoutY="144.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="projectImage" fitHeight="196.0" fitWidth="184.0" layoutX="847.0" layoutY="133.0" onMouseClicked="#showProjects" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@FXMLImage/project.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="RHRImage" fitHeight="194.0" fitWidth="184.0" layoutX="603.0" layoutY="352.0" onMouseClicked="#showRHRs" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@FXMLImage/rhr.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="PluginImage" fitHeight="184.0" fitWidth="184.0" layoutX="847.0" layoutY="352.0" onMouseClicked="#showPlugins" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@FXMLImage/plu.jpg" />
         </image>
      </ImageView>
      <ToolBar layoutX="605.0" layoutY="555.0" prefHeight="52.0" prefWidth="462.0" style="-fx-background-color: #F5F5F5;">
        <items>
          <Button fx:id="btn_setting" mnemonicParsing="false" onAction="#showSetting" prefHeight="36.0" prefWidth="109.0" style="-fx-background-color: #FFFAFA; -fx-border-color: black; -fx-text-fill: black; -fx-border-radius: 5px;" text="设置">
               <font>
                  <Font name="Dotum" size="23.0" />
               </font>
            </Button>
            <Button fx:id="btn_direction" mnemonicParsing="false" onAction="#showDirections" prefHeight="36.0" prefWidth="103.0" style="-fx-background-color: #FFFAFA; -fx-border-color: black; -fx-text-fill: black; -fx-border-radius: 5px;" text="说明">
               <font>
                  <Font name="Dotum" size="23.0" />
               </font></Button>
            <Button fx:id="btn_advertisement" mnemonicParsing="false" onAction="#showAds" prefHeight="36.0" prefWidth="103.0" style="-fx-background-color: #FFFAFA; -fx-border-color: black; -fx-text-fill: black; -fx-border-radius: 5px;" text="广告">
               <font>
                  <Font name="Dotum" size="23.0" />
               </font></Button>
            <Button fx:id="btn_feedback" mnemonicParsing="false" onAction="#showFeedback" prefHeight="36.0" prefWidth="103.0" style="-fx-background-color: #FFFAFA; -fx-border-color: black; -fx-text-fill: black; -fx-border-radius: 5px;" text="反馈">
               <font>
                  <Font name="Dotum" size="23.0" />
               </font></Button>
        </items>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </ToolBar>
      <ScrollPane fx:id="srp_tod" layoutX="14.0" layoutY="104.0" pannable="true" prefHeight="499.0" prefWidth="538.0" style="-fx-background-color: #F5F5F5; -fx-border-color: #F5F5F5;" vbarPolicy="ALWAYS">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
